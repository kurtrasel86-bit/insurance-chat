# Database
DATABASE_URL="file:./dev.db"

# OpenAI API (через AITUNNEL)
OPENAI_API_KEY="sk-aitunnel-your-key-here"
OPENAI_BASE_URL="https://api.aitunnel.ru/v1"
OPENAI_MODEL="gpt-4o-mini"

# Data Collection Settings
DATA_COLLECTION_ENABLED=true
DEFAULT_RATE_LIMIT=30

# Notification Settings
NOTIFICATION_EMAIL_ENABLED=false
NOTIFICATION_EMAIL_RECIPIENTS="admin@example.com,notifications@example.com"

NOTIFICATION_TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN="your-telegram-bot-token"
TELEGRAM_CHAT_ID="your-telegram-chat-id"

NOTIFICATION_WEBHOOK_ENABLED=false
NOTIFICATION_WEBHOOK_URL="https://your-webhook-url.com/notifications"

# Insurance Companies API Keys (if available)
SOGAZ_API_KEY=""
INGOSSTRAH_API_KEY=""
RESOGARANTIA_API_KEY=""
VSK_API_KEY=""
ROSGOSSTRAH_API_KEY=""
TINKOFF_API_KEY=""
SBERBANK_API_KEY=""
ALPHA_API_KEY=""

# Scheduler Settings
SCHEDULER_ENABLED=true
DAILY_COLLECTION_TIME="06:00"
WEEKLY_COLLECTION_DAY="0" # 0 = Sunday
MONTHLY_CONFIG_UPDATE_DAY="1"

# Web Scraping Settings
SCRAPING_TIMEOUT=30000
